<%- include("header") -%> 
<ul class="nav justify-content-end">
    <li class="nav-item">
        <a class="nav-link" style="color: black;" href="/home">Home</a>
      </li>
    <li class="nav-item">
      <a class="nav-link " style="color: black;" href="patientview">View Details</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" style="color: black;" href="patientreg">Registration</a>
    </li>
  </ul>
  
  <div class="table-container" style="padding: 20px;">
    <h1 class="heading" style="color: grey;">Patient Details</h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Age</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Address</th>

            </tr>
        </thead>
        <tbody>
          <% newPatients.forEach(function(patient){%>
            <tr>
              <td><%=patient.pid%></td>
              <td><%=patient.fname%></td>
              <td><%=patient.lname%></td>
              <td><%=patient.age%></td>
              <td><%=patient.email%></td>
              <td><%=patient.phno%></td>
              <td><%=patient.address%></td>
              
          </tr> 
            <% }) %>
           
        </tbody>
    </table>
</div>
<%- include("footer") -%>