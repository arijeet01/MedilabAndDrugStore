<%- include("header") -%> 
<ul class="nav justify-content-end">
    <li class="nav-item">
        <a class="nav-link" style="color: black;" href="/home">Home</a>
      </li>
    <li class="nav-item">
      <a class="nav-link " style="color: black;" href="staffview">View Details</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" style="color: black;" href="staffreg">Registration</a>
    </li>
  </ul>
  
  <div class="table-container" style="padding: 20px;">
    <h1 class="heading" style="color: grey;">Staff Details</h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Age</th>
                <th>Designation</th>
                <th>Address</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Joining Date</th>

            </tr>
        </thead>
        <tbody>
          <% newStaffs.forEach(function(staff){%>
            <tr>
              <td><%=staff.empid%></td>
              <td><%=staff.fname%></td>
              <td><%=staff.lname%></td>
              <td><%=staff.age%></td>
              <td><%=staff.designation%></td>
              <td><%=staff.address%></td>
              <td><%=staff.email%></td>
              <td><%=staff.phno%></td>
              <td><%=staff.jdate%></td>
              
              
          </tr> 
            <% }) %>
        </tbody>
    </table>
</div>
<%- include("footer") -%>